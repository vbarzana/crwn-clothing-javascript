  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "YarnStart",
      "cwd": "${workspaceRoot}",
      "port": 9222,
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["start"]
    },
    {
      "name": "AttachChrome",
      "type": "chrome",
      "request": "attach",
      "port": 9222,
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}/src",
      "sourceMapPathOverrides": {
        "webpack:///src/*": "${webRoot}/*"
      }
    }
  ],
  "compounds": [
    {
      "name": "Deüêûüï∑ü¶ü it...",
      "configurations": ["YarnStart", "AttachChrome"]
